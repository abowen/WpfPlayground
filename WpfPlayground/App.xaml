<Application x:Class="WpfPlayground.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wpfPlayground="clr-namespace:WpfPlayground">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <wpfPlayground:ShellBootstrapper x:Key="ShellBootstrapper" />                                     
                </ResourceDictionary>
                <ResourceDictionary Source="Styles/StackPanelStyle.xaml" />
                <ResourceDictionary Source="Styles/TextBlockStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>    
</Application>
